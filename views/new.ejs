<%- include('header'); -%>

<body class="new-page">

    <!-- Home Button -->
    <a href="/" class="home-btn">Home</a>

    <article class="notebook">

        <form class="notebook-form" action="/new" method="post" onsubmit="changeImg()">
            <!-- Tag -->
            <div class="journal-tags">
                <label>Tag</label>
                <select name="tagValue" id="tag" >
                    <option value="Wisdom">Wisdom</option>
                    <option value="Web">Web</option>
                    <option value="Problems">Problems</option>
                    <option value="Books">Books</option>
                </select>
            </div>
            <!-- Title -->
            <div class="title">
                <label for="title">Title</label>
                <input class="title-ip" value="<%= toEditArrHTML[0] %>" type="text" id="title" name="title" placeholder="Enter Title">

            </div>
            <div class="page">
                <textarea class="write-page" name="page" id="page"><%= toEditArrHTML[1] %> </textarea>
            </div>
            <button class="submit-btn" type="submit">Save</button>
        </form>
